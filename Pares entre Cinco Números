import java.util.*;
import java.lang.*;
import java.io.*;
 import java.util.Scanner;

class Main {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);
        int count = 0;

        for (int i = 0; i < 5; i++) {
            int valor = scanner.nextInt();
            if (valor % 2 == 0) {
                count++;
            }
        }

        System.out.println(count + " valores pares.");
        scanner.close();
    }
}

